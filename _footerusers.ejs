<script>
    //show profile user
    $.ajax({
        url: '/profile/users_profile',
        method: 'POST',
        success: (data) => {
            if (data.login_status == 1) {
                $('.setfullname_us').text(data.users[0].fname_us + ' ' + data.users[0].lname_us)
            } else {
                $('.setfullname_us').text(data.phone_us_nm)
            }
        }
    })
    //show history user
    $.ajax({
        url: '/history_us/list',
        method: 'POST',
        success: (data) => {
            const form_history_list = document.querySelector('.history_list .history_showlist')
            for (var i = 0; i < data.hl.length; i++) {
                const idmenufind = data.hl[i].idmenu
                const date_res = data.hl[i].date_res;
                const idres = data.hl[i].idres;
                if (data.hl[i].menu_page == 1) {
                    $.ajax({
                        url: '/history_us/name_mm',
                        method: 'POST',
                        data: { idmenufind, date_res, idres },
                        success: (mm) => {
                            const date_res = mm.date_res.split('T')
                            const format_date = date_res[0].split('-')
                            const date_th = format_date[2] + '/' + format_date[1] + '/' + (Number(format_date[0]) + 543).toString();
                            const time_res = date_res[1].split('.')
                            form_history_list.innerHTML += `<a href="/history_us/detail_hl${mm.idres}" class="border-bottom " id='chat_user_1' data-user-id='1'>
                                    <div>
                                        <h5 class="message-title mb-0 mt-1">${mm.name_mm}</h5> 
                                        <span class="fs-2 text-nowrap d-block text-muted text-truncate mt-2"></span>
                                        <span class="fs-2 text-nowrap d-block text-muted">${date_th + ' ' + time_res[0]} <span class="sl-date history_timing font-weight-medium text-info">ดูรายละเอียด</span></span>
                                    </div>
                                </a>`

                        }
                    })
                } else {
                    $.ajax({
                        url: '/history_us/name_ms',
                        method: 'POST',
                        data: { idmenufind, date_res, idres },
                        success: (ms) => {
                            const date_res = ms.date_res.split('T')
                            const format_date = date_res[0].split('-')
                            const date_th = format_date[2] + '/' + format_date[1] + '/' + (Number(format_date[0]) + 543).toString();
                            const time_res = date_res[1].split('.')
                            form_history_list.innerHTML += `<a href="/history_us/detail_hl${ms.idres}" class="border-bottom " id='chat_user_1' data-user-id='1'>
                                    <div>
                                        <h5 class="message-title mb-0 mt-1">${ms.name_ms}</h5> 
                                        <span class="fs-2 text-nowrap d-block text-muted text-truncate mt-2"></span>
                                        <span class="fs-2 text-nowrap d-block text-muted">${date_th + ' ' + time_res[0]} <span class="sl-date history_timing font-weight-medium text-info">ดูรายละเอียด</span></span>
                                    </div>
                                </a>`
                        }
                    })
                }
            }
        }
    })
</script>
