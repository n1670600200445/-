a.custom-button {
  background: transparent;
  position: relative;
  padding: 10px 20px; /* Increased padding for a more defined button */
  display: flex;
  align-items: center;
  justify-content: center; /* Center content horizontally */
  font-size: 16px; /* Reduced font-size for a sleeker look */
  font-weight: 600;
  text-decoration: none;
  cursor: pointer;
  border: 2px solid rgb(153, 255, 116); /* Increased border thickness */
  border-radius: 30px; /* Slightly more rounded corners */
  outline: none;
  overflow: hidden;
  color: rgb(153, 255, 116);
  transition: color 0.3s ease-out, background-color 0.3s ease-out; /* Added background transition */
  text-align: center;
}

a.custom-button span {
  margin: 0 12px; /* Adjusted margin for better alignment */
}

a.custom-button::before {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  content: "";
  border-radius: 50%;
  display: block;
  width: 22em;
  height: 22em;
  left: -6em;
  transition: box-shadow 0.4s ease-out, width 0.4s ease-out, height 0.4s ease-out; /* Smoother effect */
  z-index: -1;
}

a.custom-button:hover {
  color: #272020 !important;
  background-color: rgb(153, 255, 116) !important; /* Background color change on hover */
  border: 2px solid rgb(153, 255, 116); /* Ensure border consistency */
}

a.custom-button:hover::before {
  box-shadow: inset 0 0 0 12em rgb(153, 255, 116); /* Slightly faster shadow expansion */
  width: 24em; /* Dynamic hover growth */
  height: 24em;
}









